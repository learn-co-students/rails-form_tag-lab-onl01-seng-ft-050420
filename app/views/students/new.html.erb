<h2>Student Form:</h2>
<%= form_tag students_path do %>
<label>First name:</label><%= text_field_tag :'student[first_name]' %>
<label>Last name:</label><%= text_field_tag :'student[last_name]' %>
<%= submit_tag "Submit Student" %>
<%end%>

<div>
  <% @students.each do |student| %>
    <div><%= link_to student.to_s, student_path(student) %></div>
  <% end %>
</div>